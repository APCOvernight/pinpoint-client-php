{
    "name": "apc-overnight/pinpoint-client-php",
    "description": "APC Overnight PinPoint API Client Library (php).  Integrate your PHP solution with PinPoint, and track parcels being delievered by APC Overnight",
    "keywords": [
        "apc-overnight",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/APCOvernight/pinpoint-client-php",
    "license": "proprietary",
    "authors": [
        {
            "name": "APC Overnight Development Team",
            "homepage": "https://github.com/APCOvernight"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
